<h1>List of Authors</h1>
{% set x = 0 %}
{% if  tabAuthers is empty%}
<h1>Tab vide</h1>
{% else %}
<table border="1">
    <tr>
        <th>Id</th>
        <th>name</th>
        <th>email</th>
    </tr>
  {% for i in tabAuthers %}
        <tr>
            <td>{{ i.id }}</td>
            <td>{{ i.name|upper }}</td>
            <td>{{ i.email }}</td>
        </tr>
    
    {% endfor %}
   </table>
{% endif %}

