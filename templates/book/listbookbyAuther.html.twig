<table border="1" >
    <tr>
        <td>Ref</td>
        <td>Title</td>
        <td>Category</td>
        <td>Published</td>
        <td>PublicationDate</td>
        <td>Auther</td>
    </tr>
    {% for book in tabBooks %}
        <tr>
            <td>{{book.ref}}</td>
            <td>{{book.title}}</td>
            <td>{{book.category}}</td>
            <td>{{book.published}}</td>
            <td>{{book.publicationDate|date}}</td>
            <td>{{book.auther.username}}</td>
        </tr>
    {% endfor %}
</table>
